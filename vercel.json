{
    "builds":[
        {
            "src": "python_heroku_task",
            "use": "@vercel/python",
            "config":{
                "maxLambdaSize":"15mb",
                "runtime": "python3.12"
            }
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest": "python_heroku_task/wsgi.py"
        }
    ]
}
